{
    "name": "jrbconsulting/filament-menu-builder",
    "description": "Filament(v5) Menu Builder - A reusable hierarchical menu builder for Filament v5",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/jrbconsulting/filament-menu-builder",
    "keywords": ["filament", "menu", "navigation", "laravel", "filament-v5", "hierarchical"],
    "require": {
        "php": "^8.1",
        "filament/filament": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\FilamentMenuBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\FilamentMenuBuilder\\MenuBuilderServiceProvider"
            ]
        }
    }
}